{
  "name": "nodeconfessions",
  "version": "1.0.0",
  "description": "confessions bot node version",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "start-dev": "nodemon server -e html,js,scss --ignore public --ignore client",
    "seed": "node script/seed/seed",
    "deploy": "webpack git add -A",
    "heroku-prebuild": "npm install --dev",
    "retry": "git aaa && git commit -m \"npm auto update\" && npm run update && npm run heroku-log",
    "heroku-log": "heroku logs --tail",
    "update": "git push heroku master",
    "tunnel": "heroku config & heroku ps:exec",
    "heroku-off": "heroku ps:scale web=0 --app dabney-confessions",
    "heroku-on": "heroku ps:scale web=1 --app dabney-confessions"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "node-telegram-bot-api": "^0.51.0",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.24.0"
  }
}
